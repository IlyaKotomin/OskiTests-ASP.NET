@model CompliteQuestionViewModel

@{
	ViewData["Title"] = $"{Model.CorrectAnswers}/{Model.Quiz!.Questions!.Count}";
}

<div class="row">
    <div class="col-md-6 offset-3 alert alert-success text-center">
        <h2>You just finished this test!</h2>
        <p>Test: @Model.Quiz.Description</p>
        <hr />
        <p>Your mark is: @Model.CorrectAnswers/@Model.Quiz!.Questions!.Count</p>
    </div>
</div>